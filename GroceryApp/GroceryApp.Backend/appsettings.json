    "Endpoint": "https://your-computer-vision-endpoint.cognitiveservices.azure.com/",
    "SubscriptionKey": "your-subscription-key"
  },
  "OpenAI": {
    "ApiKey": "your-openai-api-key",
    "ApiUrl": "https://api.openai.com/v1/completions"
  }
